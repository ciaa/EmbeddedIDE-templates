+   UNUSED(st);
+   SET_ERR(ENOSYS);
+   return -1;
+}
+
+int _getpid_r(struct _reent *r) {
+   UNUSED(r);
+   return 1;
+}
+
+int _isatty_r(struct _reent *r, int fd) {
+   switch (fd) {
+   case 0:
+   case 1:
+   case 2:
+       return 1;
+   default:
+       SET_ERR(EBADF);
+       return -1;
+   }
+}
+
+int _kill_r(struct _reent *r, int pid, int signal) {
+   if (pid == _getpid_r(r)) {
+       switch (signal) {
+       case SIGHUP:
+       case SIGINT:
+       case SIGQUIT:
+       case SIGILL:
+       case SIGTRAP:
+       case SIGEMT:
+       case SIGFPE:
+       case SIGKILL:
+       case SIGBUS:
+       case SIGSEGV:
+       case SIGSYS:
+       case SIGPIPE:
+       case SIGALRM:
+       case SIGTERM:
+       default:
+           _exit(0);
+       }
+   } else {
+       SET_ERR(ECHILD);
+   }
+   return -1;
+}
+
+int _link_r(struct _reent *r, const char *oldf, const char *newf) {
+   UNUSED(oldf);
+   UNUSED(newf);
