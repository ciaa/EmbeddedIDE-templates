+typedef enum {
+	SPIFI_FIELDFORM_ALL_SERIAL             = 0,
+	SPIFI_FIELDFORM_SERIAL_OPCODE_ADDRESS  = 1,
+	SPIFI_FIELDFORM_SERIAL_OPCODE          = 2,
+	SPIFI_FIELDFORM_NO_SERIAL              = 3
