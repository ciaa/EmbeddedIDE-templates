+      if( uart == UART_485  ) rxIsrCallbackUART0 = rxIsrCallback;
+      if( uart == UART_USB  ) rxIsrCallbackUART2 = rxIsrCallback;
+      if( uart == UART_ENET ) rxIsrCallbackUART2 = rxIsrCallback;
+      if( uart == UART_232  ) rxIsrCallbackUART3 = rxIsrCallback;
