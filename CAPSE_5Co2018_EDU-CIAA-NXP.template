+ */
+void GPIO0_IRQHandler(void)
+{
+   serveInterrupt(0);
+}
+
+void GPIO1_IRQHandler(void)
+{
+   serveInterrupt(1);
+}
+
+void GPIO2_IRQHandler(void)
+{
+   serveInterrupt(2);
+}
