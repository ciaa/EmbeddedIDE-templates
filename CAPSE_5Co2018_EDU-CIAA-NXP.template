+#define UNUSED(x) (void)x
+#define SET_ERR(e) (r->_errno = e)
+
+void _exit(int code) {
+   register int __params__ __asm__("r0") = code;
+   while (1)
+       __asm__ __volatile__("bkpt 0");
+
+   (void) __params__;
+}
+
+int _close_r(struct _reent *r, int fd) {
