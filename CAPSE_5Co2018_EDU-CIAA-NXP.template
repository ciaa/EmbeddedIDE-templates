+/*
+ * When timer expires, ULTRASONIC_SENSOR_TRIGGER_GPIO is on
+ */
+static void timerExpired()
+{
+   gpioWrite(ULTRASONIC_SENSOR_TRIGGER_GPIO,ON);
+}
