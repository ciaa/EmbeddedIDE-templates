+void lcdSendStringRaw( char* str )
+{
+   uint8_t i = 0;
+   while( str[i] != 0 ) {
+      lcdData( str[i] );
+      i++;
+   }
+}
+
+void lcdCreateChar( uint8_t charnum, const char* chardata )
+{
+   uint8_t i;
+   charnum &= 0x07;
+   lcdCommand( E_SET_CGRAM_ADDR | (charnum << 3) );
+   for (i = 0; i < 8; i++) {
+      lcdData( chardata[i] );
+   }
+   delay(1);
+}
